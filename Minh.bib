journal = {Biopolymers},
keywords = {Alcohol Dehydrogenase,Alcohol Dehydrogenase: chemistry,Conformational-Changes,Crystallography,Drug-Resistance,Energy Landscapes,Flexibility,Flexible Proteins,HIV Protease,HIV Protease: chemistry,Hiv-1 Protease,Humans,Magnetic Resonance Spectroscopy,Molecular-Dynamics,Motions,Myoglobin,Myoglobin: chemistry,Protein Dynamics,Protein Fluctuations,Scattering,Small Angle,Solutions,Transition,Ubiquitin,Ubiquitin: chemistry,Virus Type-1 Protease,Wide-Angle X-Ray Solution Scattering,X-Ray,X-Ray Diffraction},
