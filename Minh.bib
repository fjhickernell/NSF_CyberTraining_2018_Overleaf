author = {Minh, David D. L. and Chodera, John D},
doi = {10.1063/1.3242285},
file = {:Users/dminh/Documents/Mendeley/Minh, Chodera/Journal of Chemical Physics/Minh, Chodera - 2009 - Optimal estimators and asymptotic variances for nonequilibrium path-ensemble averages.pdf:pdf},
journal = {Journal of Chemical Physics},
keywords = {Bounds,Chemical,Computer-Simulations,Empirical Distributions,Equilibrium,Fluctuation Theorem,Free-Energy Differences,Histogram Analysis Method,Models,Molecule Pulling Experiments,Monte-Carlo Integration,Reproducibility of Results,Thermodynamics},
